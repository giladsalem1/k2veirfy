stages:
  Stage 1:
    actors:
      DbCommand1:
        parent: DbCommand
        in:
          interface:
            const: POSTGRESQL_ADMIN
          sql:
            const: select * from k2verify.task_execution where task_id = ${task_id}
              order by execution_id desc
          task_id:
            external: task_id
            schema: string
            mandatory: false
            createdBy: sql
        out:
          result:
            external: result
            schema: '#ref'
schemas:
  DbCommand1.out.result:
    type: array
    items:
      type: object
      properties:
        task_id:
          type: integer
        task_title:
          type: string
        task_status:
          type: string
        task_config: {
          }
        created_by:
          type: string
        created_at:
          type: date
        updated_by:
          type: string
        updated_at:
          type: date
