  KeysMapsArr.out.array:
    type: array
    items:
      type: string
  CustomKeysArr.out.array:
    type: array
    items:
      type: string
  KeysMapsArr1.out.array:
    type: array
    items:
      type: string
  CustomKeysArr1.out.array:
    type: array
    items:
